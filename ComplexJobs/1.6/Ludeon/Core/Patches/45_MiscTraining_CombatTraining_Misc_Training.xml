				<xpath>/Defs/WorkTypeDef[defName = "MiscTraining_CombatTraining"]/naturalPriority</xpath>
				<value>
					<naturalPriority>45</naturalPriority>
				</value>
			</li>
			
		</operations>
	</Operation>
				
</Patch>