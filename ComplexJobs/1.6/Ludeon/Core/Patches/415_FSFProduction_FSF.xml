		<!-- Biotech Mechanoids Patch -->
			
			<li Class="PatchOperationConditional">		
				<xpath>/Defs/ThingDef/race/mechEnabledWorkTypes [li [text()="Hauling"] ]</xpath>
				<match Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef/race/mechEnabledWorkTypes [li [text()="Hauling"] ]</xpath>
					<value>
						<li>FSFProduction</li>
					</value>
				</match>
			</li>
			
