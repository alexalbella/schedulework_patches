		<!-- Core - Update Worktype Priority -->
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkTypeDef[defName = "Crafting"]/naturalPriority</xpath>
				<value>
					<naturalPriority>485</naturalPriority>
				</value>
			</li>
			
